    "@strapi/blocks-react-renderer": "1.0.2",
    "aos": "2.3.4",
    "astro": "5.5.3",
    "astro-icon": "1.1.5",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "embla-carousel": "8.5.2",
