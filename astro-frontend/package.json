    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-astro": "1.3.1",
    "prettier": "3.5.3",
    "tailwind-scrollbar": "3.1.0",
